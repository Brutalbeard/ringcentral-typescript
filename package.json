{
  "name": "ringcentral-typescript",
  "version": "0.3.3",
  "description": "RingCentral TypeScript SDK",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "git@github.com:tylerlong/ringcentral-typescript.git",
  "author": "Tyler Long <tyler.liu@ringcentral.com>",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "test": "jest -w 1",
    "definitions": "rm -rf ./src/definitions/* && node -r @babel/register ./code-generator/definitions.js",
    "paths": "rm -rf ./src/paths/* && node -r @babel/register ./code-generator/paths.js",
    "generate": "yarn definitions && yarn paths",
    "prepublishOnly": "tsc && yarn test"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "qs": "^6.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@types/node": "^13.11.1",
    "change-case": "^4.1.1",
    "dotenv-override-true": "^6.2.0",
    "jest": "^25.3.0",
    "lower-case-first": "^2.0.1",
    "ramda": "^0.27.0",
    "standard": "^14.3.3",
    "typescript": "^3.8.3",
    "yarn-upgrade-all": "^0.5.2"
  },
  "jest": {
    "setupFiles": [
      "dotenv-override-true/config"
    ],
    "testEnvironment": "node"
  }
}
