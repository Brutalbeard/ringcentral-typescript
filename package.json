{
  "name": "ringcentral-typescript",
  "version": "0.9.0",
  "description": "RingCentral TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:tylerlong/ringcentral-typescript.git",
  "author": "Tyler Long <tyler.liu@ringcentral.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest -w 1",
    "definitions": "rm -rf ./src/definitions/* && node ./code-generator/definitions.js",
    "paths": "rm -rf ./src/paths/* && node ./code-generator/paths.js",
    "generate": "yarn definitions && yarn paths",
    "prepublishOnly": "standard && rm -rf dist && tsc && rm dist/package.json && mv dist/src/* dist/ && rmdir dist/src && yarn test",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run check"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "form-data": "^3.0.0",
    "qs": "^6.9.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/qs": "^6.9.1",
    "change-case": "^4.1.1",
    "dotenv-override-true": "^6.2.0",
    "gts": "^2.0.0",
    "jest": "^25.5.2",
    "lower-case-first": "^2.0.1",
    "ramda": "^0.27.0",
    "standard": "^14.3.3",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3",
    "yarn-upgrade-all": "^0.5.2"
  }
}
